---
export interface Props {
  title: string;
}

const { title } = Astro.props;
import MainMenu from "../components/MainMenu.vue"

import FaviconPNG from '../images/2024/favicon.png';
import FaviconSVG from '../images/2024/favicon.svg';
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" sizes="any" href={FaviconPNG} />
    <link rel="icon" type="image/svg+xml" href={FaviconSVG} />
    <meta name="generator" content={Astro.generator} />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://js.tito.io/v2" async></script>
    
    <title>{title}</title>
    <meta name="description" content="The spiritual successor to Funconf">

    <script defer data-domain="thisnextthing.com" src="https://plausible.io/js/script.js"></script>
  </head>
  <body class="font-sans bg-cover bg-fixed">
    <div class="flex h-screen">
      <div class="m-auto">
        <slot />
      </div>
    </div>
  </body>
</html>
